<script>
// Wait until the document has finished loading before running the script
document.addEventListener('DOMContentLoaded', function() {
  // Get a reference to the product form controls group element
  const stickyCart = document.querySelector('.sticky-cart');

  // Calculate the threshold values based on the document's height
  const showThreshold = document.documentElement.scrollHeight * .23;
  const hideThreshold = document.documentElement.scrollHeight;

  // Add a scroll event listener to the window
  window.addEventListener('scroll', function() {
    // Check if the user has scrolled past the show threshold
    if (window.scrollY >= showThreshold) {
      // Show the sticky cart
      stickyCart.style.display = 'block';
    } else if (window.scrollY < hideThreshold) {
      // Hide the sticky cart
      stickyCart.style.display = 'none';
    }
  });

  // Log a message to the console to confirm that the script is running
  console.log('Scroll trigger script is running.');
});

</script>

